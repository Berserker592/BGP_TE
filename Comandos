Huawei
==============================
system-view
ipv6
interface g0/0/0
ipv6 enable
ipv6 addr 2001:a:b:7::2/64
quit
interface g0/0/1
ipv6 enable
ipv6 addr 2001:a:b:9::1/64
quit
interface g0/0/2
ipv6 enable
ipv6 addr 2001:a:b:8::2/64
quit
bgp 500
router-id 11.11.11.11
peer 2001:a:b:7::1 as-number 400
peer 2001:a:b:8::1 as-number 200
peer 2001:a:b:9::2 as-number 800
ipv6-family unicast
peer 2001:a:b:7::1 enable
peer 2001:a:b:8::1 enable
peer 2001:a:b:9::2 enable
quit
quit
===================================
JUNIPER_12
===================================
cli
configure
set interfaces em0 unit 0 family inet6 address 2001:a:b:3::2/64
set interfaces em1 unit 0 family inet6 address 2001:a:b:4::1/64
set protocols bgp bgp-identifier 12.12.12.12 local-as 300
set protocols bgp group peers-externos neighbor 2001:a:b:3::1 peer-as 200
set protocols bgp group peers-externos neighbor 2001:a:b:4::2 peer-as 800
commit

==========================================
Fortinet13
=========================================
config system global
set alias "FortiGate_13"
set hostname "FortiGate_13"
end
config system interface
edit lo0
set vdom "root"
set type loopback
config ipv6
set ip6-address fc00::13/128
end
next
edit port1
set role wan
config ipv6
set ip6-address 2001:a:b:1::1/64
end
next
edit port2
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:5::1/64
end
next
edit port3
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:10::1/64
end
next
edit port4
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:14::2/64
end
next
edit port5
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:16::1/64
end
next
edit port6
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:15::1/64
end
end
config router ospf6 
set router-id 13.13.13.13
config area
edit 0.0.0.10
end
config ospf6-interface
edit port4
set area-id 0.0.0.10 
set interface port4
next
edit port5
set area-id 0.0.0.10 
set interface port5
next
edit port6
set area-id 0.0.0.10 
set interface port6
next
edit lo0
set area-id 0.0.0.10 
set interface lo0
next
end
end
config router bgp 
set as 700
set router-id 13.13.13.13
config network6
edit 1
set prefix6 2001:a:b:26::/64
next
edit 2
set prefix6 2001:a:b:27::/64
next
edit 3
set prefix6 2001:a:b:28::/64
next
end
config neighbor
edit fc00::1 
set remote-as 700
set update-source lo0
set next-hop-self6 enable
set activate6 enable
next
edit fc00::6
set remote-as 700
set update-source lo0
set next-hop-self6 enable
set activate6 enable
next
edit fc00::4
set remote-as 700
set update-source lo0
set next-hop-self6 enable
set activate6 enable
next
edit 2001:a:b:1::2
set remote-as 100
next
edit 2001:a:b:5::2
set remote-as 400
next
edit 2001:a:b:10::2
set remote-as 600
next
end
end
config firewall policy
    edit 1
        set name "Trafico_4_1"
        set srcintf "port4"
        set dstintf "port1"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next
    edit 2
        set name "Trafico_4_2"
        set srcintf "port4"
        set dstintf "port2"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next 
    edit 3
        set name "Trafico_4_3"
        set srcintf "port4"
        set dstintf "port3"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"       
        set service "ALL"
        set logtraffic all     
    next
    edit 4
        set name "Trafico_1_4"
