==============================
Cisco_1
==============================
enable
conf ter
hostname cisco1
ipv6 unicast-routing
router ospfv3 10
router-id 1.1.1.1
exit
interface lo0
ipv6 addr fc00::1/128
ipv6 ospf 10 area 10
exit
ipv6 addr 
interface e0/0
ipv6 addr 2001:a:b:14::1/64
ipv6 ospf 10 area 10
no shut
exit
interface e0/1
ipv6 addr 2001:a:b:17::1/64
ipv6 ospf 10 area 10
no shut
exit
interface e0/2
ipv6 addr 2001:a:b:18::1/64
ipv6 ospf 10 area 10
no shut
exit
interface e0/3
ipv6 addr 2001:a:b:26::1/64
ipv6 ospf 10 area 10
no shut
exit
ipv6 dhcp pool LAN
address prefix 2001:a:b:26::/64
dns-server 2001:a:b:12::2
exit
inter e0/3
ipv6 dhcp server LAN
ipv6 nd managed-config-flag
ipv6 nd other-config-flag 
exit


router bgp 700
bgp router-id 1.1.1.1
neighbor fc00::13 remote-as 700
neighbor fc00::13 update-source Loopback0
neighbor fc00::6 remote-as 700
neighbor fc00::6 update-source Loopback0
neighbor fc00::4 remote-as 700
neighbor fc00::4 update-source Loopback0
address-family ipv6 unicast
neighbor fc00::4 activate
neighbor fc00::6 activate
neighbor fc00::13 activate
exit
