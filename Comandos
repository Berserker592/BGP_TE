========================================
MIKROTIK_9
====================================
/routing bgp instance
add as=600 disabled=no name=BGP600 router-id=9.9.9.9
/ip dhcp-client
add interface=ether1
add interface=ether4
/ipv6 address
add address=2001:a:b:10::2 interface=ether1
add address=2001:a:b:11::2 interface=ether3
add address=2001:a:b:12::1 interface=ether2
/routing bgp connection
add as=600 disabled=no instance=BGP600 local.role=ebgp name=bgp_ether1 \
    remote.address=2001:a:b:10::1/128 .as=700 routing-table=main
add as=600 disabled=no instance=BGP600 local.role=ebgp name=bgp_ether2 \
    remote.address=2001:a:b:12::2/128 .as=900 routing-table=main
add as=600 disabled=no instance=BGP600 local.role=ebgp name=bgp_ether3 \
    remote.address=2001:a:b:11::1/128 .as=400 routing-table=main
/system identity
set name=MIK9
========================================
MIKROTIK_10
====================================
/routing bgp instance
add as=800 disabled=no name=BGP800 router-id=10.10.10.10
/routing isis instance
add afi=ipv6 areas=49.0020 name=ROU_ISIS system-id=0000.0000.0010
/ip dhcp-client
add interface=ether1
# Interface not active
add interface=ether7
/ipv6 address
add address=2001:a:b:4::2 interface=ether1
add address=2001:a:b:9::2 interface=ether2
add address=2001:a:b:13::2 interface=ether3
add address=2001:a:b:22::1 interface=ether4
add address=2001:a:b:20::2 interface=ether5
add address=2001:a:b:21::1 interface=ether6
add address=fc00::10/128 advertise=no interface=lo
/ipv6 firewall address-list
add address=2001:a:b:29::/64 list=BGP_PR
add address=2001:a:b:30::/64 list=BGP_PR
add address=2001:a:b:31::/64 list=BGP_PR
/routing bgp connection
add as=800 disabled=no input.filter=FILTER_IN instance=BGP800 local.role=ebgp \
    name=bgp_ether1 output.network=BGP_PR remote.address=2001:a:b:4::1/128 \
    .as=300 routing-table=main
add as=800 disabled=no instance=BGP800 local.role=ebgp name=bgp_ether2 \
    output.network=BGP_PR remote.address=2001:a:b:9::1/128 .as=500 \
    routing-table=main
add as=800 disabled=no input.filter=FILTER_IN_2 instance=BGP800 local.role=\
    ebgp name=bgp_ether3 output.network=BGP_PR remote.address=\
    2001:a:b:13::1/128 .as=900 routing-table=main
add as=800 disabled=no instance=BGP800 local.address=fc00::10 .role=ibgp \
    name=bgp_ether5 nexthop-choice=force-self remote.address=fc00::3/128 .as=\
    800 routing-table=main
add as=800 disabled=no instance=BGP800 local.address=fc00::10 .role=ibgp \
    name=bgp_ether6 nexthop-choice=force-self remote.address=fc00::8/128 .as=\
    800 routing-table=main
add as=800 disabled=no instance=BGP800 local.address=fc00::10 .role=ibgp \
    name=bgp_ether4 nexthop-choice=force-self remote.address=fc00::14/128 \
    .as=800 routing-table=main
/routing filter rule
add chain=FILTER_IN comment="added by filter-wizard" disabled=no rule="if (dst\
    \_== 2001:a:b:26::/64 or dst == 2001:a:b:28::/64 ) { set bgp-local-pref 30\
    0; accept; }"
add chain=FILTER_IN_2 comment="added by filter-wizard" disabled=no rule=\
    "if (dst == 2001:a:b:27::/64) { set bgp-weight 800; accept; }"
/routing isis interface-template
add instance=ROU_ISIS interfaces=ether4 levels=l1
add instance=ROU_ISIS interfaces=ether5 levels=l1
add instance=ROU_ISIS interfaces=ether6 levels=l1
add instance=ROU_ISIS interfaces=lo levels=l1
/system identity
set name=MIK10

