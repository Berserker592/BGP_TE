Fortinet14
=========================================
config system global
set alias "FortiGate_14"
set hostname "FortiGate_14"
end
config system interface
edit lo0
set vdom "root"
set type loopback
config ipv6
set ip6-address fc00::14/128
end
next
edit port1
set role wan
config ipv6
set ip6-address 2001:a:b:22::2/64
end
next
edit port2
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:23::2/64
end
next
edit port3
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:25::2/64
end
next
edit port4
set mode static
set role wan
config ipv6
set ip6-address 2001:a:b:31::1/64
end
end
config router isis 
set is-type level-1
set metric-style wide
config isis-net
edit 1
set net 49.0020.0000.0000.0014.00
end
config isis-interface 
edit port1
set circuit-type level-1
set status6 enable
set network-type broadcast
next
edit port2
set circuit-type level-1
set status6 enable
set network-type broadcast
next
edit port3
set circuit-type level-1
set status6 enable
set network-type broadcast
next
edit port4
set circuit-type level-1
set status6 enable
set network-type broadcast
next
edit lo0
set circuit-type level-1
set status6 enable
next
end
end

config router bgp 
set as 800
set router-id 14.14.14.14
config neighbor
edit fc00::3 
set remote-as 800
set update-source lo0
set activate6 enable
next
edit fc00::8
set remote-as 800
set update-source lo0
set activate6 enable
next
edit fc00::10
set remote-as 800
set update-source lo0
set activate6 enable
next
end
end

config firewall policy
    edit 1
        set name "Trafico_1_4"
        set srcintf "port1"
        set dstintf "port4"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next
    edit 2
        set name "Trafico_2_4"
