H3C_6
===================================
system-view
interface LoopBack 0 
ipv6 add fc00::6/128
exit
interface g1/0
ipv6 address 2001:a:b:15::2/64
exit
interface g2/0
ipv6 address 2001:a:b:18::2/64
exit
interface g3/0
ipv6 address 2001:a:b:19::1/64
exit
interface g4/0
ipv6 address 2001:a:b:27::1/64
exit
ospfv3 10 
router-id 6.6.6.6 
area 0.0.0.10
exit
exit
interface lo 0
ospfv3 10 area 0.0.0.10
interface g1/0
ospfv3 10 area 0.0.0.10
interface g2/0
ospfv3 10 area 0.0.0.10
interface g3/0
ospfv3 10 area 0.0.0.10
interface g4/0
ospfv3 10 area 0.0.0.10
exit
bgp 700
router-id 6.6.6.6
peer fc00::1 as-number 700
peer fc00::1 source-address fc00::6
peer fc00::4 as-number 700
peer fc00::4 source-address fc00::6
peer fc00::13 as-number 700
peer fc00::13 source-address fc00::6
address-family ipv6 unicast 
peer fc00::1 enable
peer fc00::4 enable
peer fc00::13 enable
exit
exit
ipv6 dhcp pool LAN
network 2001:a:b:27::/64
dns-server 2001:a:b:12::2
exit
inter g4/0
ipv6 dhcp server apply pool LAN
ipv6 nd ra prefix default no-advertise 
ipv6 nd autoconfig managed-address-flag
ipv6 nd autoconfig other-flag 
undo ipv6 nd ra halt 
exit
