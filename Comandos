Cisco_2
==============================
enable
conf ter
hostname cisco2
ipv6 unicast-routing
interface e0/0
ipv6 addr 2001:a:b:1::2/64
no shut
exit
interface e0/1
ipv6 addr 2001:a:b:2::1/64
no shut
exit
interface e0/2
ipv6 addr 2001:a:b:6::1/64
no shut
exit
router bgp 100
bgp router-id 2.2.2.2
neighbor 2001:a:b:1::1 remote-as 700
neighbor 2001:a:b:2::2 remote-as 200
neighbor 2001:a:b:6::2 remote-as 400
address-family ipv6 unicast 
neighbor 2001:a:b:1::1 activate
neighbor 2001:a:b:2::2 activate
neighbor 2001:a:b:6::2 activate
exit
==============================
Cisco_3
==============================
enable
conf ter
hostname cisco3
ipv6 unicast-routing
router isis 10
net 49.0020.0000.0000.0003.00
metric-style wide
exit
interface lo0
ipv6 addr fc00::3/128
ipv6 router isis 10
isis circuit-type level-1
exit
interface g0/0
ipv6 addr 2001:a:b:20::1/64
ipv6 router isis 10
isis circuit-type level-1
no shut
exit
interface g0/1
ipv6 addr 2001:a:b:23::1/64
ipv6 router isis 10
isis circuit-type level-1
no shut
exit
interface g0/2
ipv6 addr 2001:a:b:24::1/64
ipv6 router isis 10
isis circuit-type level-1
no shut
exit
interface g0/3
ipv6 addr 2001:a:b:29::1/64
ipv6 router isis 10
isis circuit-type level-1
no shut
exit
ipv6 dhcp pool LAN
address prefix 2001:a:b:29::/64
dns-server 2001:a:b:13::1
exit
inter g0/3
ipv6 dhcp server LAN
ipv6 nd managed-config-flag
ipv6 nd other-config-flag 
exit
router bgp 800
bgp router-id 3.3.3.3
neighbor fc00::8 remote-as 800
neighbor fc00::8 update-source Loopback0
neighbor fc00::10 remote-as 800
neighbor fc00::10 update-source Loopback0
neighbor fc00::14 remote-as 800
neighbor fc00::14 update-source Loopback0
address-family ipv6 unicast
neighbor fc00::8 activate
neighbor fc00::10 activate
neighbor fc00::14 activate
exit
==============================
VyOS_4
==============================
configure 
set interfaces loopback lo address fc00::4/128
set interfaces ethernet eth0 address 2001:a:b:16::2/64
set interfaces ethernet eth1 address 2001:a:b:17::2/64
set interfaces ethernet eth2 address 2001:a:b:19::2/64
set interfaces ethernet eth3 address 2001:a:b:28::1/64
set protocols ospfv3 parameters router-id 4.4.4.4
set protocols ospfv3 interface eth0 area 0.0.0.10
set protocols ospfv3 interface eth1 area 0.0.0.10
set protocols ospfv3 interface eth2 area 0.0.0.10
set protocols ospfv3 interface eth3 area 0.0.0.10
set protocols ospfv3 interface lo area 0.0.0.10
set protocols bgp system-as 700
set protocols bgp parameters router-id 4.4.4.4
set protocols bgp neighbor fc00::1 remote-as 700
set protocols bgp neighbor fc00::6 remote-as 700
set protocols bgp neighbor fc00::13 remote-as 700
set protocols bgp neighbor fc00::1 address-family ipv6-unicast
set protocols bgp neighbor fc00::6 address-family ipv6-unicast
set protocols bgp neighbor fc00::13 address-family ipv6-unicast
set protocols bgp neighbor fc00::1 update-source lo 
set protocols bgp neighbor fc00::6 update-source lo 
set protocols bgp neighbor fc00::13 update-source lo

set service dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 subnet-id 28
set service  dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 interface eth3
set service  dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 option name-server 2001:a:b:12::2
set service dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 range clientes start 2001:a:b:28::2
set service dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 range clientes stop 2001:a:b:28::ffff
set service dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 lease-time maximum 86400
set service dhcpv6-server shared-network-name LAN subnet 2001:a:b:28::/64 lease-time minimum 3600
set service router-advert interface eth3 managed-flag 
set service router-advert interface eth3 other-config-flag
commit
==============================
VyOS_5
==============================
configure 
set interfaces ethernet eth0 address 2001:a:b:2::2/64
set interfaces ethernet eth1 address 2001:a:b:3::1/64
set interfaces ethernet eth2 address 2001:a:b:8::1/64
set protocols bgp system-as 200
set protocols bgp parameters router-id 5.5.5.5
set protocols bgp neighbor 2001:a:b:2::1 remote-as 100
set protocols bgp neighbor 2001:a:b:3::2 remote-as 300
set protocols bgp neighbor 2001:a:b:8::2 remote-as 500
set protocols bgp neighbor 2001:a:b:2::1 address-family ipv6-unicast
set protocols bgp neighbor 2001:a:b:3::2 address-family ipv6-unicast
set protocols bgp neighbor 2001:a:b:8::2 address-family ipv6-unicast
commit
