        set dstintf "port2"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next 
end  
config router prefix-list6
    edit "PL_LAN_29"
        config rule
            edit 1
                set prefix6 2001:a:b:29::/64
                set action permit
            next
        end
    next
    edit "PL_LAN_30"
        config rule
            edit 1
                set prefix6 2001:a:b:30::/64
                set action permit
            next
        end
    next
    edit "PL_LAN_31"
        config rule
            edit 1
                set prefix6 2001:a:b:31::/64
                set action permit
            next
        end
    next
end	

config router route-map
    edit "RM_M_LP"
        config rule
            edit 1
                set match-ip6-address "PL_LAN_29"
                set set-local-preference 300
            next
        end
    next
    edit "RM_M_WEIGHT"
        config rule
            edit 1
                set match-ip6-address "PL_LAN_30"
                set set-weight 800
            next
        end
    next
    edit "RM_M_ASPATH"
        config rule
            edit 1
                set match-ip6-address "PL_LAN_31"
                set set-aspath-action prepend
                set set-aspath "700 700 700"
            next
        end
    next
end

config router bgp
    config neighbor
        edit "2001:a:b:1::2"
            set route-map-in6 "RM_M_LP"
        next
        edit "2001:a:b:10::2"
            set route-map-in6 "RM_M_WEIGHT"
        next
        edit "2001:a:b:5::2"
            set route-map-in6 "RM_M_ASPATH"
        next
    end
end

execute router clear bgp all
