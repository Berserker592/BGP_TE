        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next 
    edit 6
        set name "Trafico_3_4"
        set srcintf "port3"
        set dstintf "port4"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next 
    edit 7
        set name "Trafico_6_1"
        set srcintf "port6"
        set dstintf "port1"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"
        set schedule "always"
        set service "ALL"
        set logtraffic all
    next 
    edit 8
        set name "Trafico_6_2"
        set srcintf "port6"
        set dstintf "port2"
        set action accept
        set srcaddr6 "all"
        set dstaddr6 "all"        
        set schedule "always"
        set service "ALL"
